{
  "name": "teamusscrapper",
  "version": "1.0.2",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "dev": "NODE_ENV=development npm-run-all postinstall start",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf dist/*",
    "postinstall": "npm-run-all clean tsc copy-assets",
    "tsc": "tsc",
    "copy-assets": "ts-node tools/copyAsset",
    "test": "nyc --reporter=html --reporter=text ./node_modules/.bin/mocha  ./tests/*.test.js --require @babel/register --timeout 7000ms --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start:dev": "nodemon --watch ./ -e ts,pug,css --exec npm run dev"
  },
  "engines": {
    "node": "11.14.0"
  },
  "keywords": [],
  "author": "Daniel Charles Mwangila",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truestbyheart/TeamUsScrapper"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cheerio": "^0.22.13",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.45",
    "@types/mongodb": "^3.3.5",
    "@types/mongoose": "^5.5.19",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.11",
    "@types/shelljs": "^0.8.5",
    "fs-extra": "^8.1.0",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "shelljs": "^0.8.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "http": "0.0.0",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "pug": "^2.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  }
